{
  "name": "multiflip",
  "version": "4.0.0",
  "description": "Animation of dom, flipping its sections for showing and hiding the whole",
  "main": "multiflip.js",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "test": "open test.html",
    "dist": "browserify -t babelify src.js > multiflip.js",
    "prepublish": "npm run dist",
    "lint": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kt3k/multiflip.git"
  },
  "keywords": [
    "jquery-plugin",
    "animation",
    "flip",
    "flipping",
    "dom"
  ],
  "author": "Yoshiya Hinosawa <stibium121@gmail.com> (https://twitter.com/kt3k)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kt3k/multiflip/issues"
  },
  "homepage": "https://github.com/kt3k/multiflip#readme",
  "peerDependencies": {
    "capsid": ">=0.3.0",
    "jquery": ">=2.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "capsid": "^0.2.3",
    "jquery": "^3.1.1",
    "standard": "^8.6.0"
  },
  "standard": {
    "ignore": [
      "multiflip.js"
    ],
    "globals": [
      "jQuery"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
